{
  "name": "golden-tetris-bot",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "shx rm -rf ./dist",
    "build": "tsc",
    "dev": "nodemon src/server.ts",
    "rebuild": "yarn clean && yarn build",
    "start": "yarn rebuild && node --harmony ./dist/server.js",
    "start:dev": "cross-env NODE_ENV=dev && yarn start",
    "watch": "yarn rebuild && nodemon -e ts -w ./src -x yarn watch:serve",
    "watch:serve": "node --harmony ./dist/server.js"
  },
  "dependencies": {
    "koa": "^2",
    "koa-bodyparser": "^4",
    "koa-router": "^8",
    "koa-send": "^5.0.0",
    "telegraf": "^3.38.0"
  },
  "devDependencies": {
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.0",
    "@types/koa-send": "^4.1.2",
    "nodemon": "^2",
    "shx": "^0",
    "ts-node": "^8",
    "tsconfig-paths": "^3",
    "typescript": "^3"
  }
}
